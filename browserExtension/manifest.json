{
    "manifest_version": 3,
    "name": "Reviewlhino",
    "description": "This extension will help you to detect fake reviews on ecom",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
        "24": "icons/icon24.png",
        "128": "icons/icon128.png"
      },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
  }